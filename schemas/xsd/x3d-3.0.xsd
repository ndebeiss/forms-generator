	<xsd:complexType name="X3DNetworkSensorNode" abstract="true" mixed="false">
		<xsd:annotation>
			<xsd:appinfo/>
			<xsd:documentation source="http://www.web3D.org/technicalinfo/specifications/vrml97/part1/concepts.html#4.6.7.2"/>
