			<xsd:element ref="GeoLocation"/>
			<xsd:element ref="GeoLOD"/>
			<xsd:element ref="GeoMetadata"/>
			<xsd:element ref="GeoOrigin"/>
			<xsd:element ref="GeoPositionInterpolator"/>
